    void setup() {
    
    int hafta=1;

    switch ( hafta){

      case 1:
      Serial.print("PAZARTESI");
      break;
      case 2 :
      Serial.print("SALI");
      break;
      case 3:

      Serial.print("CARSAMBA");
      break;
      case 4 :

      Serial.print("PERSEMBE");
      break;
      case 5 :

      Serial.print("CUMA");
      break;
      case 6 :

      Serial.print("CUMARTESI");
      break;
      case 7 :
      
      Serial.print("PAZAR");
      break;
      default;
      Serial.print("Gecersiz bir gun girdiniz.")
      break;
    }
    Serial.println();
    hafta++

    }

