void setup() {
 for(int port = 0;port<14;port++){

   pinMode(port,OUTPUT);
 }

}

void loop() {
 // for( int i =0; i<14; i++){
  //  digitalWrite(i,HIGH);
 // }
//delay(100);
//for( int i =0; i<14 ;i++){

  //digitalWrite(i,LOW);

//}
//delay(100);

for (int i =0 ;i<14 ;i++) {

digitalWrite(i,HIGH);
delay(20);
digitalWrite(i,LOW);
delay(20);
}

for (int i =13;i>0;i--){
  digitalWrite(i,HIGH);
  delay(20);
  digitalWrite(i,LOW);
  delay(20);
}
}
